<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.autclub">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.autclub.AppModel.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainController.MainActivity" />
        <activity
            android:name="com.example.autclub.MainController.NewsfeedActivity"
            android:parentActivityName="com.example.autclub.MainController.MainActivity" />
        <activity
            android:name="com.example.autclub.ClubController.ClubListPageActivity"
            android:parentActivityName="com.example.autclub.MainController.MainActivity" />
        <activity
            android:name="com.example.autclub.ClubController.ClubPageActiviy"
            android:parentActivityName="com.example.autclub.ClubController.ClubListPageActivity" />
        <activity
            android:name="com.example.autclub.MainController.ViewActivity"
            android:parentActivityName="com.example.autclub.ClubController.ClubListPageActivity" />
        <activity
            android:name=".MainController.ChatActivity"/>
        <activity android:name="com.example.autclub.MainController.Report" />
        <activity android:name="com.example.autclub.InitialController.InstructionPage" />
        <activity android:name="com.example.autclub.LoginSignupController.ResetPasswordActivity" />
        <activity android:name="com.example.autclub.MainController.NotificationActivity" />
        <activity android:name="com.example.autclub.LoginSignupController.SignUpActivity" />
        <activity android:name="com.example.autclub.LoginSignupController.LoginActivity" />
        <activity android:name="com.example.autclub.InitialController.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>